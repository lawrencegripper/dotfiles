{
    "files.associations": {
        "*.hujson": "jsonc"
    },
    "json.schemas": [{
        "fileMatch": ["*.hujson"],
        "schema": {
            "allowTrailingCommas": true
        }
    }],
    "terminal.integrated.profiles.linux": {
        "zsh": {
            "path": "zsh"
        },
    },
    "rust-analyzer.completion.limit": 12000,
    "terminal.integrated.defaultProfile.linux": "zsh",
    // "terminal.integrated.sendKeybindingsToShell": true,
    "editor.semanticHighlighting.enabled": true,
    "editor.semanticTokenColorCustomizations": {
        "[Arc Dark Theme]": {
            "enabled": true
        }
    },
    // "rubyTestExplorer.debugCommand": "rdebug-ide",
    "rubyTestExplorer.logpanel": true,
    "workbench.colorTheme": "Ayu Mirage Bordered",
    "terminal.integrated.copyOnSelection": true,
    "remote.SSH.showLoginTerminal": true,
    "yaml.schemas": {
        // "kubernetes": "*.yaml",
    },
    "yaml.completion": true,
    "yaml.schemaStore.enable": true,
    "zenMode.centerLayout": false,
    "zenMode.hideLineNumbers": false,
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "workbench.sideBar.location": "right",
    "languageToolLinter.external.url": "http://localhost:8123",
    "languageToolLinter.serviceType": "external",
    "languageToolLinter.languageTool.language": "en-GB",
    "languageToolLinter.languageTool.motherTongue": "en-GB",
    "languageToolLinter.lintOnOpen": true,
    "languageToolLinter.lintOnChange": true,
    "dotfiles.repository": "lawrencegripper/dotfiles",
    "dotfiles.targetPath": "~/dotfiles",
    "dotfiles.installCommand": "~/dotfiles/script/bootstrap",
    "github.codespaces.defaultExtensions": [
        "davidlday.languagetool-linter",
        "GitHub.codespaces",
        "GitHub.vscode-pull-request-github",
        "eamodio.gitlens",
        "oderwat.indent-rainbow",
        "redhat.vscode-yaml",
        "github.copilot",
        "humao.rest-client",
        "oderwat.indent-rainbow",
        "github.vscode-pull-request-github",
        "mutantdino.resourcemonitor",
        "itarato.byesig",
        "aaron-bond.better-comments",
        "adpyke.codesnap",
        "GitHub.copilot-chat@prerelease",
        "GitHub.copilot-nightly",
        "SangsooNam.vscode-extension-quick-grep",
        "anykeyh.simplecov-vscode"
    ],
    "dev.containers.defaultExtensions": [
        "davidlday.languagetool-linter",
        "eamodio.gitlens",
        "oderwat.indent-rainbow",
        "redhat.vscode-yaml",
        "github.copilot",
        "humao.rest-client",
        "oderwat.indent-rainbow",
        "github.vscode-pull-request-github",
        "mutantdino.resourcemonitor",
        "itarato.byesig",
        "aaron-bond.better-comments",
        "adpyke.codesnap",
        "GitHub.copilot-chat@prerelease",
        "GitHub.copilot-nightly",
        "SangsooNam.vscode-extension-quick-grep"
    ],
    "editor.fontFamily": "'Monaspace Neon', monospace",
    "editor.inlineSuggest.fontFamily": "'Monaspace Krypton', monospace",
    // "editor.fontFamily": "FiraMono Nerd Font Mono",
    // "editor.fontFamily": "'Monaspace Krypton', monospace",
    // "editor.fontFamily": "'Droid Sans Mono', 'monospace', monospace",
    // "editor.fontFamily": "Monaspace Argon Frozen",
    "editor.fontLigatures": "'calt', 'liga', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'ss09'",
    // "editor.fontLigatures": true,
    "terminal.integrated.fontSize": 14,
    "editor.fontSize": 14,
    "terminal.integrated.scrollback": 100000,
    "ruby.useLanguageServer": false,
    "ruby.lint": {
        "rubocop": true
    },

    "[ruby]": {
        "editor.formatOnSave": true,
        "editor.detectIndentation": false,
        "editor.tabSize": 2
    },
    // "ruby.interpreter.commandPath": "bin/safe-ruby-quick",
    // "ruby.pathToBundler": "bin/bundle",
    "ruby.useBundler": false,
    // "solargraph.commandPath": "bin/solargraph",
    "solargraph.checkGemVersion": false,
    // "go.delveConfig": {
    //     "dlvLoadConfig": {
    //         "followPointers": true,
    //         "maxVariableRecurse": 1,
    //         "maxStringLen": 8192,
    //         "maxArrayValues": 64,
    //         "maxStructFields": -1
    //     },
    //     "apiVersion": 2,
    //     "showGlobalVariables": true
    // },
    // "go.testFlags": [
    //     "-v"
    // ],
    "go.lintFlags": [
    ],
    "go.lintTool": "golangci-lint",
    // "go.lintFlags": [],
    "go.lintOnSave": "workspace",
    "go.coverageOptions": "showUncoveredCodeOnly",
    "go.coverageDecorator": {
        "type": "highlight"
        // "coveredHighlightColor": "rgba(64,128,128,0.5)",
        // "uncoveredHighlightColor": "rgba(128,64,64,0.25)",
        // "coveredGutterStyle": "blockgreen",
        // "uncoveredGutterStyle": "blockred"
    },
    "go.coverOnSingleTest": true,
    "[go]": {
        "editor.insertSpaces": false,
        "editor.autoClosingBrackets": "never",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
        "editor.suggest.snippetsPreventQuickSuggestions": false
    },
    "shellcheck.enableQuickFix": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "git-rebase-todo": "default"
    },
    "editor.renderWhitespace": "trailing",
    "workbench.editor.enablePreview": true,
    "sqltools.useNodeRuntime": true,
    "jupyter.askForKernelRestart": false,
    "editor.inlineSuggest.enabled": true,
    "redhat.telemetry.enabled": false,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "github.copilot.enable": {
        "*": false,
        "plaintext": true,
        "markdown": false,
        "scminput": false,
        "yaml": true,
        "jsonc": false,
        "ruby": true,
        "python": true,
        "shellscript": true,
        "rust": true,
        "go": false
    },
    "explorer.confirmDragAndDrop": false,
    "languageToolLinter.languageTool.ignoredWordsGlobal": [
        "dotcom",
        "proxima"
    ],
    "go.toolsManagement.autoUpdate": true,
    "workbench.startupEditor": "none",
    "diffEditor.ignoreTrimWhitespace": false,
    "comments.openView": "never",
    // "terminal.integrated.enableMultiLinePasteWarning": false,
    "githubPullRequests.pullBranch": "never",
    "byesig.opacity": 0.4,
    "byesig.showIcon": false,
    "window.zoomLevel": 0,
    "security.workspace.trust.untrustedFiles": "open",
    // "editor.formatOnSave": true,
    // "editor.formatOnSaveMode": "file",
    "[python]": {
        "editor.formatOnType": true,
    },
    "python.analysis.typeCheckingMode": "basic",
    "editor.stickyScroll.enabled": true,
    "git.openRepositoryInParentFolders": "always",
    "indentRainbow.colors": [
        "rgba(38, 70, 83,0.15)",
        "rgba(42, 157, 143,0.15)",
        "rgba(233, 196, 106, 0.15)",
        "rgba(244, 162, 97,0.15)",
        "rgba(231, 111, 81,0.15)",
        "rgba(231, 111, 81,0.1)",
        "rgba(231, 111, 81,0.05)",
        "rgba(231, 111, 81,0.025)"
    ],
    "editor.guides.bracketPairs": true,
    "workbench.iconTheme": "Monokai Pro Icons",
    "testExplorer.useNativeTesting": true,
    "githubPullRequests.createOnPublishBranch": "never",
    "quickgrep.preview": false,
    "quickgrep.grep": "Git Grep",
    "remote.SSH.connectTimeout": 360,
    "github.copilot.editor.enableAutoCompletions": true,
    "workbench.colorCustomizations": {
        "[Material Theme]": {
            "activityBarBadge.background": "#64FFDA",
            "activityBar.activeBorder": "#64FFDA",
            "list.activeSelectionForeground": "#64FFDA",
            "list.inactiveSelectionForeground": "#64FFDA",
            "list.highlightForeground": "#64FFDA",
            "scrollbarSlider.activeBackground": "#64FFDA50",
            "editorSuggestWidget.highlightForeground": "#64FFDA",
            "textLink.foreground": "#64FFDA",
            "progressBar.background": "#64FFDA",
            "pickerGroup.foreground": "#64FFDA",
            "tab.activeBorder": "#64FFDA",
            "notificationLink.foreground": "#64FFDA",
            "editorWidget.resizeBorder": "#64FFDA",
            "editorWidget.border": "#64FFDA",
            "settings.modifiedItemIndicator": "#64FFDA",
            "settings.headerForeground": "#64FFDA",
            "panelTitle.activeBorder": "#64FFDA",
            "breadcrumb.activeSelectionForeground": "#64FFDA",
            "menu.selectionForeground": "#64FFDA",
            "menubar.selectionForeground": "#64FFDA",
            "editor.findMatchBorder": "#64FFDA",
            "selection.background": "#64FFDA40",
            "statusBarItem.remoteBackground": "#64FFDA"
        }
    },
    "materialTheme.accent": "Bright Teal",
    "rubyLsp.enabledFeatures": {
        "codeActions": true,
        "diagnostics": true,
        "documentHighlights": true,
        "documentLink": true,
        "documentSymbols": true,
        "foldingRanges": true,
        "formatting": false,
        "hover": true,
        "inlayHint": true,
        "onTypeFormatting": true,
        "selectionRanges": true,
        "semanticHighlighting": true,
        "completion": false,
        "codeLens": true,
        "definition": false,
        "workspaceSymbol": false,
        "signatureHelp": true,
        "typeHierarchy": true
    },
    "dotnet.completion.showCompletionItemsFromUnimportedNamespaces": true,
    "window.menuBarVisibility": "toggle",
    "chat.editing.alwaysSaveWithGeneratedChanges": true,
	"remote.autoForwardPortsSource": "hybrid",
	"extensions.trustedPublishers": [
		"eamodio"
    ],
    "gitlens.views.scm.grouped.views": {
        "commits": false,
        "branches": false,
        "remotes": false,
        "stashes": false,
        "tags": false,
        "worktrees": false,
        "contributors": false,
        "repositories": false,
        "searchAndCompare": false,
        "launchpad": false
    },
    "git.blame.editorDecoration.enabled": true,
    "github.copilot.nextEditSuggestions.enabled": false
}
