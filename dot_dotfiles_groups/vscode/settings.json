{
    "files.associations": {
        "*.hujson": "jsonc"
    },
    "json.schemas": [{
        "fileMatch": ["*.hujson"],
        "schema": {
            "allowTrailingCommas": true
        }
    }],
    "terminal.integrated.profiles.linux": {
        "zsh": {
            "path": "zsh"
        },
    },
    "rust-analyzer.completion.limit": 12000,
    "terminal.integrated.defaultProfile.linux": "zsh",
    // "terminal.integrated.sendKeybindingsToShell": true,
    "editor.semanticHighlighting.enabled": true,
    "editor.semanticTokenColorCustomizations": {
        "[Arc Dark Theme]": {
            "enabled": true
        }
    },
    // "rubyTestExplorer.debugCommand": "rdebug-ide",
    "rubyTestExplorer.logpanel": true,
    "workbench.colorTheme": "Monokai Pro (Filter Spectrum)",
    "terminal.integrated.copyOnSelection": true,
    "remote.SSH.showLoginTerminal": true,
    "yaml.schemas": {
        "kubernetes": "*.yaml",
    },
    "yaml.completion": true,
    "yaml.schemaStore.enable": true,
    "zenMode.centerLayout": false,
    "zenMode.hideLineNumbers": false,
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "workbench.sideBar.location": "right",
    "languageToolLinter.external.url": "http://localhost:8123",
    "languageToolLinter.serviceType": "external",
    "languageToolLinter.languageTool.language": "en-GB",
    "languageToolLinter.languageTool.motherTongue": "en-GB",
    "languageToolLinter.lintOnOpen": true,
    "languageToolLinter.lintOnChange": true,
    "dotfiles.repository": "lawrencegripper/dotfiles",
    "dotfiles.targetPath": "~/dotfiles",
    "dotfiles.installCommand": "~/dotfiles/script/bootstrap",
    "github.codespaces.defaultExtensions": [
        "davidlday.languagetool-linter",
        "GitHub.codespaces",
        "GitHub.vscode-pull-request-github",
        "eamodio.gitlens",
        "oderwat.indent-rainbow",
        "redhat.vscode-yaml",
        "github.copilot",
        "humao.rest-client",
        "oderwat.indent-rainbow",
        "github.vscode-pull-request-github",
        "mutantdino.resourcemonitor",
        "itarato.byesig",
        "aaron-bond.better-comments",
        "adpyke.codesnap",
        "GitHub.copilot-chat@prerelease",
        "GitHub.copilot-nightly",
        "SangsooNam.vscode-extension-quick-grep",
        "anykeyh.simplecov-vscode"
    ],
    "dev.containers.defaultExtensions": [
        "davidlday.languagetool-linter",
        "eamodio.gitlens",
        "oderwat.indent-rainbow",
        "redhat.vscode-yaml",
        "github.copilot",
        "humao.rest-client",
        "oderwat.indent-rainbow",
        "github.vscode-pull-request-github",
        "mutantdino.resourcemonitor",
        "itarato.byesig",
        "aaron-bond.better-comments",
        "adpyke.codesnap",
        "GitHub.copilot-chat@prerelease",
        "GitHub.copilot-nightly",
        "SangsooNam.vscode-extension-quick-grep"
    ],
    "editor.fontFamily": "'Monaspace Neon', monospace",
    "editor.fontLigatures": true,
    "terminal.integrated.fontFamily": "FiraCode Nerd Font Mono",
    "terminal.integrated.fontSize": 16,
    "editor.fontSize": 16,
    "terminal.integrated.scrollback": 100000,
    "ruby.useLanguageServer": false,
    "ruby.lint": {
        "rubocop": true
    },

    "[ruby]": {
        "editor.formatOnSave": true,
        "editor.detectIndentation": false,
        "editor.tabSize": 2
    },
    // "ruby.interpreter.commandPath": "bin/safe-ruby-quick",
    // "ruby.pathToBundler": "bin/bundle",
    "ruby.useBundler": false,
    // "solargraph.commandPath": "bin/solargraph",
    "solargraph.checkGemVersion": false,
    // "go.delveConfig": {
    //     "dlvLoadConfig": {
    //         "followPointers": true,
    //         "maxVariableRecurse": 1,
    //         "maxStringLen": 8192,
    //         "maxArrayValues": 64,
    //         "maxStructFields": -1
    //     },
    //     "apiVersion": 2,
    //     "showGlobalVariables": true
    // },
    // "go.testFlags": [
    //     "-v"
    // ],
    "go.lintTool": "golangci-lint",
    // "go.lintFlags": [],
    "go.lintOnSave": "workspace",
    "go.coverOnSave": true,
    "go.coverageOptions": "showUncoveredCodeOnly",
    "go.coverageDecorator": {
        "type": "highlight"
        // "coveredHighlightColor": "rgba(64,128,128,0.5)",
        // "uncoveredHighlightColor": "rgba(128,64,64,0.25)",
        // "coveredGutterStyle": "blockgreen",
        // "uncoveredGutterStyle": "blockred"
    },
    "go.coverOnSingleTest": true,
    "[go]": {
        "editor.insertSpaces": false,
        "editor.autoClosingBrackets": "never",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
        "editor.suggest.snippetsPreventQuickSuggestions": false
    },
    "shellcheck.enableQuickFix": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "git-rebase-todo": "default"
    },
    "editor.renderWhitespace": "trailing",
    "workbench.editor.enablePreview": true,
    "sqltools.useNodeRuntime": true,
    "jupyter.askForKernelRestart": false,
    "editor.inlineSuggest.enabled": true,
    "redhat.telemetry.enabled": false,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": false,
        "scminput": false,
        "yaml": true,
        "jsonc": false,
        "ruby": true,
        "python": true,
        "shellscript": true,
        "rust": true
    },
    "explorer.confirmDragAndDrop": false,
    "languageToolLinter.languageTool.ignoredWordsGlobal": [
    ],
    "go.toolsManagement.autoUpdate": true,
    "workbench.startupEditor": "none",
    "diffEditor.ignoreTrimWhitespace": false,
    "comments.openView": "never",
    "window.menuBarVisibility": "toggle",
    // "terminal.integrated.enableMultiLinePasteWarning": false,
    "githubPullRequests.pullBranch": "never",
    "byesig.opacity": 0.4,
    "byesig.showIcon": false,
    "window.zoomLevel": 3,
    "security.workspace.trust.untrustedFiles": "open",
    // "editor.formatOnSave": true,
    // "editor.formatOnSaveMode": "file",
    "[python]": {
        "editor.formatOnType": true,
    },
    "python.analysis.typeCheckingMode": "basic",
    "editor.stickyScroll.enabled": true,
    "git.openRepositoryInParentFolders": "always",
    "indentRainbow.colors": [
        "rgba(38, 70, 83,0.15)",
        "rgba(42, 157, 143,0.15)",
        "rgba(233, 196, 106, 0.15)",
        "rgba(244, 162, 97,0.15)",
        "rgba(231, 111, 81,0.15)",
        "rgba(231, 111, 81,0.1)",
        "rgba(231, 111, 81,0.05)",
        "rgba(231, 111, 81,0.025)"
    ],
    "editor.guides.bracketPairs": true,
    "workbench.iconTheme": "Monokai Pro (Filter Spectrum) Icons",
    "testExplorer.useNativeTesting": false,
    "githubPullRequests.createOnPublishBranch": "never",
    "simplecov-vscode.enabled": false,
    "quickgrep.preview": false,
    "quickgrep.grep": "Ripgrep",
    "remote.SSH.connectTimeout": 360
}
